# Using this file -> make .env and .env.staging

# Database connection
TYPEORM_NAME=default
TYPEORM_HOST=
TYPEORM_TYPE=mysql
TYPEORM_USERNAME=root
TYPEORM_PASSWORD=
TYPEORM_DATABASE=
TYPEORM_PORT=3306
TYPEORM_ENTITIES=/**/*.entity{.ts,.js}

# Never use automatic sync in production (CHANGE TRUE -> FALSE)
TYPEORM_MIGRATIONS_RUN=true
TYPEORM_SYNCHRONIZE=true
